    "# https://www.codewars.com/kata/585d7d5adb20cf33cb000235\n",
    "\n",
    "from collections import Counter\n",
    "\n",
    "def find_uniq(arr):\n",
    "    x = dict(Counter(arr))\n",
    "    i = list(x.values()).index(1)\n",
    "    return list(x.keys())[i]"
