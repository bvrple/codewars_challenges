    " # https://www.codewars.com/kata/514b92a657cdc65150000006\n",
    " \n",
    "def solution(n):\n",
    "    nums = []\n",
    "    x = range(0,n)\n",
    "    for i in x:\n",
    "        if i %3 == 0 or i %5 == 0:\n",
    "            nums.append(i)\n",
    "    return sum(nums)"
