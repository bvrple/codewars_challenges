    "# https://www.codewars.com/kata/520b9d2ad5c005041100000f\n",
    "\n",
    "def pig_it(text):\n",
    "    words = text.split(' ')\n",
    "    pig = []\n",
    "\n",
    "    for item in words:\n",
    "        if item.isalpha():\n",
    "            pig.append(item[1:]+item[0]+'ay')\n",
    "        else:\n",
    "            pig.append(item)\n",
    "    return ' '.join(pig)"
   ]
