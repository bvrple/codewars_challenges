    "\n",
    "def save(sizes, hd): \n",
    "    x = 0\n",
    "    y = 0\n",
    "    for i in sizes:\n",
    "        if x+i <= hd:\n",
    "            x+=i\n",
    "            y+=1\n",
    "            print(x)\n",
    "        else:\n",
    "            break    \n",
    "    return y"
