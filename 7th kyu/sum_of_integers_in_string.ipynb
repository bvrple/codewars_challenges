    "# https://www.codewars.com/kata/598f76a44f613e0e0b000026\n",
    "\n",
    "import re\n",
    "\n",
    "def sum_of_integers_in_string(s):\n",
    "    nums = re.findall('(\\d+)', s)\n",
    "    return sum([int(x) for x in nums])"
