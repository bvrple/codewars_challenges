    "# https://www.codewars.com/kata/515de9ae9dcfc28eb6000001\n",
    "\n",
    "def solution(s):\n",
    "    splits = []\n",
    "    n = 2\n",
    "    if len(s)%2!=0:\n",
    "            s = s + '_'\n",
    "    for index in range(0, len(s), n):\n",
    "        splits.append(s[index : index + n])\n",
    "    return(splits)"
