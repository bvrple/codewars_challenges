    "# https://www.codewars.com/kata/556e0fccc392c527f20000c5\n",
    "\n",
    "def Xbonacci(sig,n):\n",
    "    if n < len(sig):\n",
    "        return sig[:n]\n",
    "    else:\n",
    "        for i in range(0, n-len(sig)):\n",
    "            sig.append(sum(sig[i:]))\n",
    "        return sig"
